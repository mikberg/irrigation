{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "rootDirs": [
      ".",
    ],
    "baseUrl": ".",
    "paths": {
      "@irrigation/*": [
        "./*",
        "./bazel-out/host/bin/*",
        "./bazel-out/darwin-fastbuild/bin/*",
      ]
    }
  },
}
